[program:{{ application_name }}]
command = bash {{ project_path }}/gunicorn_start.sh            ; Command to start app
user = {{ gunicorn_user }}                                            ; User to run as
stdout_logfile = {{ application_log_dir }}/okapi_gunicorn_supervisor.log  ; Where to write log messages
redirect_stderr = true                                                ; Save stderr in the same log
environment=LANG=en_US.UTF-8,LC_ALL=en_US.UTF-8                       ; Set UTF-8 as default encoding
